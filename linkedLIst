LINKED LIST-------



#include<iostream>

using namespace std;
class Node{
    public:
    int data;
    Node* next;


    Node(int val){     // constructor
        data = val;
        next = NULL;
    }
};

class list{
public:
    Node* head;
    Node* tail;
    
  list(){

    head = NULL;
    tail = NULL;
  }
void push_front(int val){
Node* newNode=new Node(val);
if(head==NULL){
    head = tail = newNode;
}
else{
    newNode->next = head;
    head = newNode;
}
}
     
     void pop_front(){
         
         if (head==NULL){
             return;
         }
    Node* temp=head;
    head=head->next;
    if(head==NULL)
    tail=NULL;
         
     }
     
    void printList(){
        Node* temp = head;
        while(temp != NULL){
            cout<<temp->data<<"->";
            temp = temp -> next;
        }cout<<"NULL\n";
    }
};
int main(){

    list ll;
    ll.push_front(2);
    ll.push_front(3);
    ll.push_front(4);
    ll.pop_front();
    ll.printList();

    return 0;
}


